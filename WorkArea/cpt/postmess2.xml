vcommand = "cmd /C echo %USERNAME%"
activate "OSSHELL".commandout(vcommand,USER)
$PID$ = $processinfo("pid")
$appltitle = "%%$instancename - PID %%$PID$ "
$TARGET$ = "RECVMESS1"

; Many starup shells use a static /UST=ustname command line to register with 
; Urouter. The open command below registers this runtime program with Urouter 
; dynamically.  This UST name comes from the  
; current PID.  The $DNP path relies on the value of $default_net, which
; defaults to port 13001, but should be set expicitly in the ASN file.

open "localhost|%%USER%%%||%%$PID$%%%","$DNP"
activate "UROUTMON".connect(HOSTNAME.E1,"TCP",UR_PORT.E1)
activate "UROUTMON".get_clients("C","","","","URCLIENTS")
MESSAGE.E1 = "Message Utility initiated from %%$instancename/%%$PID$ at %%$Datim "

edit



trigger accept
#ifdefined frmEntity
  ; Generated three tier component: Delegate save() to FRM Subtype.
  ; This allows a uniform implementation that can be invoked
  ; by other objects too (for example, by an 'OK' Button).
if ($formmod)
  $collhandle("&lt;frmEntity&gt;")-&gt;save()       ; save modifications
  if ($status &lt; 0)                         ; check negative status from Save()
    return (-1)
  endif
endif

exit (0)
#else
#endif



trigger erase
#ifdefined frmEntity
  ; Generated three tier component: Delegate deleteall() to FRM Subtype.
  ; This allows a uniform implementation that can be invoked
  ; by other objects too (for example, by a 'Delete All' Button).
  $collhandle("&lt;frmEntity&gt;")-&gt;deleteall()

#else
  erase
  if ($status &lt; 0)
    message $text(1763)   ;error
    rollback
  else
    if ($status = 1)
      message $text(1634) ;not allowed
    else
      message $text(1806) ;Ok
      commit
    endif
  endif

#endif



trigger store
#ifdefined frmEntity
  ; Generated three tier component: Delegate save() to FRM Subtype.
  ; This allows a uniform implementation that can be invoked
  ; by other objects too (for example, by a 'Save' Button).
  $collhandle("&lt;frmEntity&gt;")-&gt;save()

#else
  store
  if ($status &lt; 0)
     message $text(1500)    ; error
     rollback
  else
    if ($status = 1)
      message $text(1723)   ; no change
    else
      commit
      if ($status &lt; 0)
        rollback
      else
        message $text(1805) ; Ok
      endif
    endif
  endif

#endif



