<DAT name="U_DTYP">S</DAT>
<DAT name="U_INDB">X</DAT>
<DAT name="INTERFACEMOD">C40</DAT>
<DAT name="WIDGETTYPE">COMMANDBUTTON</DAT>
<DAT name="UINHERIT">F</DAT>
<DAT name="DETAIL" xml:space='preserve'>variables
   String vCommand, vDnp, vDst, vInst, vNetwork, vTarget
endvariables

if (UST="her")
  vInst = ""
else
  vInst = $instancename
endif

$6 = $split(HOST,1,"/",vDnp)
vDst = "%%UST%%%:"
vNetwork = "%%vDnp%%%+%%UR_PORT.E1%%%|%%USER%%%"
vTarget = "%%vDst%%%%%vInst%%%"

activate "POSTMESSDLL".UPOSTMESSAGE(vNetwork,vTarget,"DLLSIG",MESSAGE)

MESSAGE.E1 = "Message Utility updated at %%$Datim from %%$instancename"
MESSAGE.E1 = "%%MESSAGE.E1%%%/%%$SOURCEHOST$ "</DAT>
