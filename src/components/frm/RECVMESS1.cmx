<DAT name="UECTRIGGERS" xml:space='preserve'>; -- start extended triggers for grid widget 

trigger ColumnHeader_LClicked 
params 
  string fieldName: in 
  numeric shiftKey: in 
endparams 
; sort occurrences based on the clicked column
if ($sortOrder$ = "%%fieldName%%%:a")
   $sortOrder$ = "%%fieldName%%%:d"
else
   $sortOrder$ = "%%fieldName%%%:a"
endif

sort/e "&lt;$entname&gt;", $sortOrder$
$prompt = fieldName

end ; trigger ColumnHeader_LClicked 

trigger RowHeader_LClicked 
params 
  numeric rowNumber : in 
  numeric shiftKey  : in 
endparams 

; trap the user choosing a UROUTMON or WEB client which cannot process
; these messages.  They are indetified by using ().  Thus this edit isn't
; completely foolproof.

setocc $entname, rowNumber
if ($scan(UST,"(")) message/error "Choose a UST that belongs to a real client"
putmess "row selected is %%HOST %%UST "
end ; trigger RowHeader_LClicked 

trigger CornerButton_LClicked 
  params 
  numeric shiftKey  : in 
endparams 

end ; trigger CornerButton_LClicked 

trigger VScrollBar_HitEnd  

end ; trigger VScrollBar_HitEnd 

; -- end triggers for grid widget 
</DAT>
